
public class Coordinate {
	protected int x;
	protected int y;
	protected int counter;
	public Coordinate() {
		x = 0;
		y = 0;
		counter = 0;
	}
	public Coordinate(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public Coordinate(int x, int y, int c) {
		this.x = x;
		this.y = y;
		this.c = counter;
	}
}
